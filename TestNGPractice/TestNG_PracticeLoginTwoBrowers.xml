<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parameters Two Browsers TestSuite" parallel="tests" thread-count="2">
<!--//MUST RUN UNDER TestNG 6.11-->
<test name="Test Firefox">
	<parameter name="browser" value="Firefox"></parameter>
	<classes>
		<class name="testclasses.TestNG_PracticeLoginTwoBrowers"></class>
	</classes>
</test>
<test name="Test Chrome">
	<parameter name="browser" value="chrome"></parameter>
	<classes>
		<class name="testclasses.TestNG_PracticeLoginTwoBrowers"></class>
	</classes>
</test>
</suite>